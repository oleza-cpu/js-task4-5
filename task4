    <script type="text/javascript">
      function calc(a, b, op) {
    switch (op) {
        case 1:
            return a - b;
        case 2:
            return a * b;
        case 3:
            return a / b;
        default:
            return a + b;
    }
}


console.log(calc(6, 2, 3)); 
console.log(calc(5, 3, 1)); 
console.log(calc(5, 3, 2)); 
console.log(calc(2, 3, 4)); 





        </script>
</body>

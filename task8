    <script type="text/javascript">
     function createArrayIterator(array) {
    let index = 0; 

    function iterate() {
        if (index < array.length) {
            return array[index++];
        } else {
            return undefined; 
        }
    }

    return iterate;
}


var arr = [5, 3, 7];
var itr = createArrayIterator(arr);
console.log(itr()); 
console.log(itr()); 
console.log(itr()); 
console.log(itr()); 





        </script>

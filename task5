    <script type="text/javascript">
      function digitN(k, n) {
    
    if (k < 0 || !Number.isInteger(k)) {
        return -1;
    }

    
    const digitsCount = String(k).length;
    if (n > digitsCount || n <= 0) {
        return -1;
    }

    
    const digit = Number(String(k)[digitsCount - n]);
    return digit;
}


console.log(digitN(12345, 2)); 
console.log(digitN(987654321, 5)); 
console.log(digitN(123, 4)); 






        </script>
</body>
